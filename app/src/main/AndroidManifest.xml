<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shruti.moodleapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_appicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AppBase"
            android:label="@string/title_activity_admin" />
        <activity
            android:name=".AddStudent"
            android:label="@string/title_activity_add_student"
            android:parentActivityName=".AppBase" />
        <activity
            android:name=".AddFaculty"
            android:label="@string/title_activity_add_faculty"
            android:parentActivityName=".AppBase" />
        <activity android:name=".ClassData" />
        <activity
            android:name=".Faculty"
            android:label="FACULTY" />
        <activity android:name=".ClassAdd" />
        <activity android:name=".Questions_add" />
        <activity android:name=".Questions_edit" />
        <activity android:name=".Quiz_edit" />
        <activity android:name=".Quiz_create" />
        <activity android:name=".TestQuiz" />
        <activity android:name=".MaterialUpload"></activity>
        <activity android:name=".AttendanceSheet"></activity>

    </application>

</manifest>