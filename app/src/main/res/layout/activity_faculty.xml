    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.shruti.moodleapp.Faculty"
    android:background="@color/colorPrimaryDark">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
